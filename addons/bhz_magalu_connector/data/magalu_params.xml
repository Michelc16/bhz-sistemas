      <field name="key">bhz_magalu.client_id</field>
      <field name="value">PASTE_YOUR_CLIENT_ID_HERE</field>
    </record>

    <record id="bhz_magalu_param_client_secret" model="ir.config_parameter">
      <field name="key">bhz_magalu.client_secret</field>
      <field name="value">PASTE_YOUR_CLIENT_SECRET_HERE</field>
    </record>

    <!-- como vocÃª registrou os 2 redirects no Magalu, escolhi o sem www -->
    <record id="bhz_magalu_param_redirect" model="ir.config_parameter">
      <field name="key">bhz_magalu.redirect_uri</field>
      <field name="value">https://bhzsistemas.com.br/magalu/oauth/callback</field>
    </record>
  </data>
</odoo>
