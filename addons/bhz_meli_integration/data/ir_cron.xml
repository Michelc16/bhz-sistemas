<odoo>
    <data noupdate="0">
        <record id="ir_cron_meli_fetch_orders" model="ir.cron">
            <field name="name">BHZ ML: Buscar pedidos</field>
            <field name="model_id" ref="model_meli_order"/>
            <field name="state">code</field>
            <field name="code">model.cron_fetch_orders()</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="active">True</field>
            <field name="nextcall" eval="(DateTime.now() + relativedelta(minutes=2)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

        <record id="ir_cron_meli_fetch_items" model="ir.cron">
            <field name="name">BHZ ML: Buscar an√∫ncios</field>
            <field name="model_id" ref="model_meli_product"/>
            <field name="state">code</field>
            <field name="code">model.cron_fetch_items()</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="active">True</field>
            <field name="nextcall" eval="(DateTime.now() + relativedelta(minutes=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>
    </data>
</odoo>
