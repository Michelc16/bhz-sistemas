<odoo>
  <record id="view_delivery_carrier_form_bhz" model="ir.ui.view">
    <field name="name">delivery.carrier.form.bhz</field>
    <field name="model">delivery.carrier</field>
    <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
    <field name="arch" type="xml">
      <xpath expr="//group[@name='provider_details']//field[@name='delivery_type']" position="after">
        <field name="bhz_provider"/>
      </xpath>
    </field>
  </record>
</odoo>
