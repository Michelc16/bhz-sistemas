        <record id="privilege_bhz_city_places" model="res.groups.privilege">
            <field name="name">BHZ Locais</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_bhz_place_manager" model="res.groups">
            <field name="name">BHZ Locais - Gerente</field>
            <field name="privilege_id" ref="bhz_city_places.privilege_bhz_city_places"/>
        </record>

        <record id="group_bhz_place_editor" model="res.groups">
            <field name="name">BHZ Locais - Editor</field>
            <field name="privilege_id" ref="bhz_city_places.privilege_bhz_city_places"/>
            <field name="implied_ids" eval="[(4, ref('group_bhz_place_manager'))]"/>
        </record>
    </data>
</odoo>
